@Article{Cullen-JMLA2002,
 author =  {RJ Cullen},
 title =   {In search of evidence: family practitioners' use of the Internet for clinical information},
 journal = {J Med Libr Assoc},
 year =    {2002},
 volume =  {90},
 number =  {4},
 pages =   {370-9},
 ngr =     {The aim of the study was to determine the extent of use of the Internet for clinical information among family practitioners in New Zealand, their skills in accessing and evaluating this information, and the ways they dealt with patient use of information from the Internet. RES: More than 80\% (294/363) of members in the sample completed and returned the questionnaire. Of these, 48.6\% reported that they used the Internet to look for clinical information. Gender and age were more significant in determining use than practice type or location. Information was primarily sought on rare diseases, updates on common diseases, diagnosis, and information for patients. MEDLINE was the most frequently accessed source. Search skills were basic, and abstracts were commonly used if the full text of an item was not readily available. Most reported that up to 10\% of patients bring information from the Internet to consultations. Both Internet users and non-Internet users encouraged patients to search the Web. Internet users were more likely to recommend specific sites.}
}

